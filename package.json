{
  "name": "angular-cosmosdb",
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "dev-proxy":
      "concurrently \"npm run debug\" \"ng serve --proxy-config proxy.conf.json --open\"",
    "ng": "ng",
    "start-ng": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod --build-optimizer",
    "sme": "./node_modules/.bin/source-map-explorer",
    "gen-stats": "ng build --prod --stats-json",
    "view-stats": "webpack-bundle-analyzer dist/publicweb/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docker-up": "docker-compose up -d --build",
    "docker-up-debug": "docker-compose -f docker-compose.debug.yml up -d --build",
    "docker-down": "docker-compose down",
    "start": "PUBLICWEB=./dist/publicweb node -r dotenv/config src/server/index.js",
    "debug": "PUBLICWEB=./dist/publicweb node -r dotenv/config --inspect src/server/index.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "body-parser": "^1.18.2",
    "core-js": "^2.4.1",
    "csurf": "~1.9.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.0",
    "express-session": "~1.15.5",
    "helmet": "~3.8.1",
    "lusca": "~1.5.1",
    "mongoose": "~4.11.1",
    "passport": "~0.4.0",
    "passport-twitter": "~1.0.4",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.6.7",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "source-map-explorer": "^1.5.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
